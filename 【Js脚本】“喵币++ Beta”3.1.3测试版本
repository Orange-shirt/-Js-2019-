            toastLog("æ£€æµ‹åˆ°æ­£ç¡®çš„Activity,ç»§ç»­è¿è¡Œâ€¦â€¦");
        } else {
            var deng_rightActivity = 10;
            for (deng_rightActivity == 10; deng_rightActivity > 0; deng_rightActivity--) {
                toastLog("click_quliulan2():\næ£€æµ‹ä¸åˆ°å¯¹çš„Activity\nç­‰å¾…" + deng_rightActivity + "ç§’åå°†é‡æ–°æ£€æµ‹â€¦â€¦\nè¯·ä¸è¦åŠ¨æ‰‹æœºï¼å½“å‰Activity\n" + currentActivity());
                sleep(2000);
            }
            if (currentActivity() == "com.taobao.browser.BrowserActivity") {
                toastLog("æ£€æµ‹åˆ°æ­£ç¡®çš„Activity,ç»§ç»­è¿è¡Œâ€¦â€¦");
            } else {
                dialogs.alert("(Ë˜ï½¥_ï½¥Ë˜)\næ£€æµ‹ä¸åˆ°æ­£ç¡®çš„Activity", "è¿™å¯èƒ½æ˜¯ç”±äºè¯¯è§¦å¯¼è‡´çš„ï¼Œåœ¨è„šæœ¬è¿è¡Œæ—¶è¯·ä¸è¦è¿›è¡Œä»»ä½•æ“ä½œ\n\næ‚¨å¯ä»¥å°è¯•é‡æ–°è¿è¡Œä¸€æ¬¡ï¼Œç‚¹å‡»ç¡®å®šåœæ­¢è¿è¡Œè„šæœ¬ï¼\nå¯¼è‡´é”™è¯¯çš„Activityâ†“\n" + currentActivity());
                engines.stopAllAndToast();
            }
        }*/
    if (text("å»æµè§ˆ").exists()) {
        while (text("å»æµè§ˆ").exists()) {
            text("å»æµè§ˆ").findOne().click();
            sleep(5000);
            toastLog("å½“å‰ä¸ºæµè§ˆ15ç§’çš„é¡µé¢");
            sleep(4000);
            swipe(width / 2, height - 500, width / 2, 0, 500);
            sleep(2500);
            swipe(width / 2, height - 500, width / 2, 0, 500);
            sleep(2500);
            swipe(width / 2, height - 500, width / 2, 0, 500);
            sleep(2500);
            swipe(width / 2, height - 500, width / 2, 0, 500);
            sleep(10000);
            swipe(width / 2, height - 500, width / 2, 0, 500);
            sleep(1000); //è¿™ä¸ªåœ°æ–¹å¦‚æœç½‘ç»œä¸å¥½ï¼Œè‡ªå·±åŠ æ—¶é—´(å•ä½æ˜¯æ¯«ç§’)ï¼Œ1000æ¯«ç§’ä¸º1ç§’   

            //ğŸ’›ğŸ’›ğŸ’›ğŸ’›ğŸ’›â£ï¸â£ï¸â£ï¸â£ï¸
            if (currentActivity() == "com.taobao.tao.TBMainActivity") {
                app.startActivity({
                    packageName: "com.taobao.taobao",
                    className: "com.taobao.search.searchdoor.SearchDoorActivity"
                });
                toastLog("è‹¥é•¿æ—¶é—´æ²¡æœ‰æ‰“å¼€æ·˜å®\nè¯·è‡ªè¡Œæ‰“å¼€æ·˜å®è‡³æœç´¢ç•Œé¢ï¼");
                waitForActivity("com.taobao.search.searchdoor.SearchDoorActivity");
                sleep(1000);
                setText("åŒåä¸€åˆä¼™äºº");
                if (text("æœç´¢").exists()) {
                    text("æœç´¢").findOne().click();
                }
                jinhuodong();
                click_mp();
            } else {
                //ğŸ’ğŸ’ğŸ’ğŸ’ğŸ’ä½¿ç”¨ç”¨æˆ·è®¾å®šçš„è¿”å›æ–¹æ³•
                if (context_i_back == 0) {
                    sleep(1000);
                    toastLog("ä½¿ç”¨æ™®é€šçš„è¿”å›");
                    back();
                    sleep(2000);
                }
                if (context_i_back == 1) {
                    sleep(1000);
                    toastLog("ä½¿ç”¨ROOTè¿”å›\nè¯·ç¡®ä¿å·²ç»™ROOTæƒé™ï¼");
                    Back();
                    sleep(2000);
                }
                if (context_i_back == 2) {
                    sleep(1000);
                    toastLog("ä»å±å¹•ä¸­é—´å‘ä»å·¦å‘å†…æ»‘åŠ¨æ¥è¿”å›");
                    gestures([context_gestures_speed, [0, height / 2],
                        [500, height / 2]
                    ]);
                    sleep(2000);
                }
                if (context_i_back == 4) {
                    sleep(1000);
                    toastLog("ä»å±å¹•å·¦ä¾§ä¸‹æ–¹å‘ä¸Šæ»‘åŠ¨æ¥è¿”å›");
                    gestures([context_gestures_speed, [width / 2 - 300, height - 1],
                        [width / 2 - 300, height - 500]
                    ]);
                    sleep(2000);
                }
                if (context_i_back == 3) {
                    sleep(1000);
                    toastLog("ä»å±å¹•ä¸­é—´å‘ä»å³å‘å†…æ»‘åŠ¨æ¥è¿”å›");
                    gestures([context_gestures_speed, [width - 1, height / 2],
                        [width - 500, height / 2]
                    ]);
                    sleep(2000);
                }
                if (context_i_back == 5) {
                    sleep(1000);
                    toastLog("ä»å±å¹•å·¦ä¾§ä¸‹é¢å‘ä¸Šé¢æ»‘åŠ¨æ¥è¿”å›");
                    gestures([context_gestures_speed, [width / 2 + 300, height - 1],
                        [width / 2 + 300, height - 500]
                    ]);
                    sleep(2000);
                }
                sleep(2000);
            }
        }
    }
}

function click_quchakan() {
    sleep(1000);
    if (text("å»æŸ¥çœ‹").exists()) {
        text("å»æŸ¥çœ‹").findOne().click();
        sleep(3000);
        //ğŸ’ğŸ’ğŸ’ğŸ’ğŸ’ä½¿ç”¨ç”¨æˆ·è®¾å®šçš„è¿”å›æ–¹æ³•
        if (context_i_back == 0) {
            sleep(1000);
            toastLog("ä½¿ç”¨æ™®é€šçš„è¿”å›");
            back();
            sleep(2000);
        }
        if (context_i_back == 1) {
            sleep(1000);
            toastLog("ä½¿ç”¨ROOTè¿”å›\nè¯·ç¡®ä¿å·²ç»™ROOTæƒé™ï¼");
            Back();
            sleep(2000);
        }
        if (context_i_back == 2) {
            sleep(1000);
            toastLog("ä»å±å¹•ä¸­é—´å‘ä»å·¦å‘å†…æ»‘åŠ¨æ¥è¿”å›");
            gestures([context_gestures_speed, [0, height / 2],
                [500, height / 2]
            ]);
            sleep(2000);
        }
        if (context_i_back == 4) {
            sleep(1000);
            toastLog("ä»å±å¹•å·¦ä¾§ä¸‹æ–¹å‘ä¸Šæ»‘åŠ¨æ¥è¿”å›");
            gestures([context_gestures_speed, [width / 2 - 300, height - 1],
                [width / 2 - 300, height - 500]
            ]);
            sleep(2000);
        }
        if (context_i_back == 3) {
            sleep(1000);
            toastLog("ä»å±å¹•ä¸­é—´å‘ä»å³å‘å†…æ»‘åŠ¨æ¥è¿”å›");
            gestures([context_gestures_speed, [width - 1, height / 2],
                [width - 500, height / 2]
            ]);
            sleep(2000);
        }
        if (context_i_back == 5) {
            sleep(1000);
            toastLog("ä»å±å¹•å·¦ä¾§ä¸‹é¢å‘ä¸Šé¢æ»‘åŠ¨æ¥è¿”å›");
            gestures([context_gestures_speed, [width / 2 + 300, height - 1],
                [width / 2 + 300, height - 500]
            ]);
            sleep(2000);
        }
        //è¿”å›å®Œæˆâœ…
        sleep(2000);
    }
}


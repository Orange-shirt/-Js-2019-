function Justback() {
    //ğŸ’ğŸ’ğŸ’ğŸ’ğŸ’ä½¿ç”¨ç”¨æˆ·è®¾å®šçš„è¿”å›æ–¹æ³•
    if (context_i_back == 0) {
        sleep(1000);
        toastLog("ä½¿ç”¨æ™®é€šçš„è¿”å›");
        back();
        sleep(2000);
    }
    if (context_i_back == 1) {
        sleep(1000);
        toastLog("ä½¿ç”¨ROOTè¿”å›\nè¯·ç¡®ä¿å·²ç»™ROOTæƒé™ï¼");
        Back();
        sleep(2000);
    }
    if (context_i_back == 2) {
        sleep(1000);
        toastLog("ä»å±å¹•ä¸­é—´å‘ä»å·¦å‘å†…æ»‘åŠ¨æ¥è¿”å›");
        gestures([context_gestures_speed, [0, height / 2],
            [500, height / 2]
        ]);
        sleep(2000);
    }
    if (context_i_back == 4) {
        sleep(1000);
        toastLog("ä»å±å¹•å·¦ä¾§ä¸‹æ–¹å‘ä¸Šæ»‘åŠ¨æ¥è¿”å›");
        gestures([context_gestures_speed, [width / 2 - 300, height - 1],
            [width / 2 - 300, height - 500]
        ]);
        sleep(2000);
    }
    if (context_i_back == 3) {
        sleep(1000);
        toastLog("ä»å±å¹•ä¸­é—´å‘ä»å³å‘å†…æ»‘åŠ¨æ¥è¿”å›");
        gestures([context_gestures_speed, [width - 1, height / 2],
            [width - 500, height / 2]
        ]);
        sleep(2000);
    }
    if (context_i_back == 5) {
        sleep(1000);
        toastLog("ä»å±å¹•å·¦ä¾§ä¸‹é¢å‘ä¸Šé¢æ»‘åŠ¨æ¥è¿”å›");
        gestures([context_gestures_speed, [width / 2 + 300, height - 1],
            [width / 2 + 300, height - 500]
        ]);
        sleep(2000);
    }
}

function main_js_clickAll15s() {
    if (text("å»ç­¾åˆ°").exists()) {
        text("å»ç­¾åˆ°").findOne().click();
        sleep(2000);
    }
    while (textContains("å»æµè§ˆ").exists()) {
        textContains("å»æµè§ˆ").findOne().click();
        var dengdai10s = 10;
        for (dengdai10s == 10; dengdai10s > 0; dengdai10s--) {
            toastLog("æ­£åœ¨å®Œæˆâ€œå»æµè§ˆâ€ä»»åŠ¡\nå‰©ä½™" + dengdai10s + "ç§’â€¦â€¦")
            sleep(2000);
            swipe(width / 2, height - 600, width / 2, 0, 500);
        }
        if (currentActivity() == "com.taobao.tao.TBMainActivity") {
            app.startActivity({
                packageName: "com.taobao.taobao",
                className: "com.taobao.search.searchdoor.SearchDoorActivity"
            });
            toastLog("è‹¥é•¿æ—¶é—´æ²¡æœ‰æ‰“å¼€æ·˜å®æœç´¢\nè¯·è‡ªè¡Œæ‰“å¼€æ·˜å®è‡³æœç´¢ç•Œé¢ï¼");
            waitForActivity("com.taobao.search.searchdoor.SearchDoorActivity");
            sleep(1000);
            setText("åŒåäºŒå…¨æ°‘å¯»å®");
            if (text("æœç´¢").exists()) {
                text("æœç´¢").findOne().click();
            }
            jinhuodong();
            click_powerË‰qiandao();
        } else {
            Justback();
        }
    }
    while (textContains("å»æœç´¢").exists()) {
        textContains("å»æœç´¢").findOne().click();
        var dengdai10s = 10;
        for (dengdai10s == 10; dengdai10s > 0; dengdai10s--) {
            toastLog("æ­£åœ¨å®Œæˆâ€œå»æµè§ˆâ€ä»»åŠ¡\nå‰©ä½™" + dengdai10s + "ç§’â€¦â€¦")
            sleep(2000);
            swipe(width / 2, height - 600, width / 2, 0, 500);
        }
        Justback();
    }
}


    sleep(1000);
    toastLog("ç”±äºç‚¹å‡»å–µé“ºå¯èƒ½ä¼šå‡çº§\nå› æ­¤æ‚¨éœ€è¦ç­‰"+jishi_8s+"ç§’");
    }
if (text("å¼€å¿ƒæ”¶ä¸‹").exists()){
        sleep(5000);
        text("å¼€å¿ƒæ”¶ä¸‹").findOne().click();
        //å¼€å¿ƒæ”¶ä¸‹ç»§ç»­ç‚¹å‡»æˆ‘çš„å–µå¸
        textContains("æˆ‘çš„å–µå¸").waitFor()
         text("æˆ‘çš„å–µå¸").findOne().click();
     if (text("å¼€å¿ƒæ”¶ä¸‹").exists()){
        sleep(5000);
        text("å¼€å¿ƒæ”¶ä¸‹").findOne().click();
        //å¼€å¿ƒæ”¶ä¸‹ç»§ç»­ç‚¹å‡»æˆ‘çš„å–µå¸
